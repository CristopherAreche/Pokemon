# Supabase (client + server)
NEXT_PUBLIC_SUPABASE_URL="https://YOUR_PROJECT_REF.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY"
SUPABASE_SERVICE_ROLE_KEY="YOUR_SUPABASE_SERVICE_ROLE_KEY"

# PostgreSQL (Supabase)
DATABASE_URL="postgresql://postgres:YOUR_PASSWORD_ENCODED@db.YOUR_PROJECT_REF.supabase.co:5432/postgres?pgbouncer=true&connection_limit=1"
DIRECT_URL="postgresql://postgres:YOUR_PASSWORD_ENCODED@db.YOUR_PROJECT_REF.supabase.co:5432/postgres"

# App
NEXTAUTH_SECRET="CHANGE_ME"
NEXTAUTH_URL="http://localhost:3000"
NEXT_PUBLIC_API_URL="http://localhost:3000/api"

# Admin endpoint protection (required for POST/DELETE/refresh=true)
ADMIN_API_KEY="CHANGE_ME_TO_A_STRONG_SECRET"

# Optional: only for local admin UI usage; do not use this in public production clients
NEXT_PUBLIC_ADMIN_API_KEY="CHANGE_ME_TO_THE_SAME_VALUE_AS_ADMIN_API_KEY"

